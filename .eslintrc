{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["jest", "prettier", "react"],
  "globals": {
    "Promise": true
  },
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-uses-vars": "error"
  }
}
